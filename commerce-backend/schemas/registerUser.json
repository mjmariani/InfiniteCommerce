{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "chrome2x",
            "password": "pass",
            "first_name": "Kevin",
            "last_name": "Markson",
            "email": "markson@gmail.com",
            "is_admin": "False"
        }
    ],
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "chrome2x"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pass"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The firstName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Kevin"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The lastName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Markson"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "markson@gmail.com"
            ],
            "title": "The email schema",
            "type": "string",
            "format": "email"
        },
        "isAdmin": {
            "$id": "#/properties/is_admin",
            "default": "",
            "description": "boolean value of isAdmin; not required",
            "examples": [
                "False"
            ],
            "title": "The isAdmin schema",
            "type": "boolean"
            
        }
    },
    "additionalProperties": true
}